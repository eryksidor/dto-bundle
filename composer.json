{
  "name": "codelab/dto-bundle",
  "type": "symfony-bundle",
  "description": "Bundle for Data transfer objects",
  "license": "MIT",
  "require": {
    "php": ">=8.1.0",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "sensio/framework-extra-bundle": "^6.2",
    "symfony/validator": "6.0.*"
  },
  "autoload": {
    "psr-4": {
      "CodeLab\\DtoBundle\\": "/"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "CodeLab\\DtoBundle\\Tests\\": "/Tests"
    }
  }
}
